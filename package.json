{
  "name": "sqltemple",
  "version": "0.0.2",
  "description": "A modern, AI-powered SQL IDE built with Electron and React.",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "typecheck": "tsc --noEmit",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "postinstall": "electron-rebuild -f -w better-sqlite3",
    "prepare": "husky",
    "test": "jest",
    "test:unit": "jest src/main/__tests__/unit",
    "test:integration": "jest src/main/__tests__/integration",
    "test:e2e": "jest src/main/__tests__/e2e",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "config": {
    "forge": "./forge.config.js"
  },
  "keywords": [],
  "repository": {
    "url": "https://github.com/vicotrbb/sqltemple"
  },
  "author": {
    "name": "Victor Bona",
    "email": "victor.bona@hotmail.com",
    "url": "https://victorbona.dev"
  },
  "license": "Apache 2.0",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-dmg": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-webpack": "^7.8.1",
    "@electron-forge/publisher-github": "^7.8.3",
    "@jest/globals": "^30.1.2",
    "@timfish/forge-externals-plugin": "^0.2.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.30",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "autoprefixer": "^10.4.21",
    "buffer": "^6.0.3",
    "css-loader": "^7.1.2",
    "electron": "^36.4.0",
    "electron-forge": "^7.8.1",
    "electron-rebuild": "^3.2.9",
    "electron-squirrel-startup": "^1.0.1",
    "events": "^3.3.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-mock-extended": "^4.0.0",
    "lint-staged": "^16.1.6",
    "mini-css-extract-plugin": "^2.9.2",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "nock": "^14.0.10",
    "postcss": "^8.5.4",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.6.2",
    "process": "^0.11.10",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.61.0",
    "@monaco-editor/react": "^4.7.0",
    "better-sqlite3": "^11.10.0",
    "electron-updater": "^6.6.2",
    "mermaid": "^11.6.0",
    "monaco-editor": "^0.52.2",
    "openai": "^5.2.0",
    "pg": "^8.16.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ]
  }
}
